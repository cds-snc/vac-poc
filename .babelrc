{
  "presets": [
    [
      "env",
      {
        "targets": {
          "browsers": [">0.25%", "ie 11", "not op_mini all"]
        },
        "useBuiltIns": true
      }
    ],
    "react"
  ],
  "env": {
    "development": {
      "plugins": [["emotion", { "sourceMap": true, "autoLabel": true}]],
      "presets": ["flow", "next/babel"]
    },
    "production": {
      "plugins": [["emotion", { "hoist": true}], "transform-inline-environment-variables"],
      "presets": ["next/babel"]
    },
    "test": {
      "presets": [["next/babel", { "preset-env": { "modules": "commonjs" } }]]
    }
  }
}